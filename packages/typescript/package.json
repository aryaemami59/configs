    "./base/with-js": "./base/with-js/tsconfig.json",
    "./Base/with-js": "./base/with-js/tsconfig.json",
    "./bundler": "./bundler/esnext/tsconfig.json",
    "./Bundler": "./bundler/esnext/tsconfig.json",
    "./bundler/with-js": "./bundler/esnext/with-js/tsconfig.json",
    "./Bundler/with-js": "./bundler/esnext/with-js/tsconfig.json",
    "./bundler/esnext": "./bundler/esnext/tsconfig.json",
    "./Bundler/esnext": "./bundler/esnext/tsconfig.json",
    "./Bundler/ESNext": "./bundler/esnext/tsconfig.json",
    "./bundler/esnext/with-js": "./bundler/esnext/with-js/tsconfig.json",
    "./Bundler/esnext/with-js": "./bundler/esnext/with-js/tsconfig.json",
