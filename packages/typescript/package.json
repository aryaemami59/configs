{
  "name": "@aryaemami59/tsconfig",
  "version": "0.0.5",
  "description": "TypeScript configuration",
  "keywords": [
    "config",
    "tsconfig",
    "typescript",
    "ts",
    "configuration"
  ],
  "homepage": "https://github.com/aryaemami59/configs/tree/master/packages/typescript#readme",
  "bugs": {
    "url": "https://github.com/aryaemami59/configs/issues"
  },
  "repository": {
