{
  "name": "@arya/prettier-config",
  "version": "0.0.1",
  "description": "Prettier configuration",
  "types": "./dist/prettier.config.d.mts",
  "module": "./dist/prettier.config.mjs",
  "main": "./dist/prettier.config.mjs",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/prettier.config.d.mts",
        "default": "./dist/prettier.config.mjs"
      },
      "node": {
        "types": "./dist/prettier.config.d.mts",
        "default": "./dist/prettier.config.mjs"
      }
    }
  },
  "files": [
    "dist",
    "*.mts"
  ],
  "scripts": {
    "build": "yarn clean && tsc",
    "clean": "rimraf dist",
    "prepack": "yarn build"
  },
  "keywords": [
    "prettier",
    "config",
    "prettier-config",
    "configuration"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3"
  }
}
