{
  "name": "@arya/eslint-config",
  "version": "0.0.2",
  "description": "ESLint configuration for TypeScript projects",
  "author": "Arya Emami <aryaemami59@yahoo.com> (https://github.com/aryaemami59)",
  "license": "MIT",
  "types": "./dist/eslint.config.d.mts",
  "main": "./dist/eslint.config.mjs",
  "module": "./dist/eslint.config.mjs",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/eslint.config.d.mts",
      "import": "./dist/eslint.config.mjs",
      "default": "./dist/eslint.config.mjs"
    }
  },
  "files": [
    "dist",
    "*.mts"
  ],
  "scripts": {
    "build": "yarn clean && tsc",
    "clean": "rimraf dist",
    "prepack": "yarn build"
  },
  "keywords": [
    "eslint",
    "config",
    "eslint-config",
    "configuration"
  ],
  "publishConfig": {
    "access": "public"
