{
  "name": "@aryaemami59/eslint-config",
  "version": "0.0.2",
  "description": "ESLint configuration for TypeScript projects",
  "author": "Arya Emami <aryaemami59@yahoo.com> (https://github.com/aryaemami59)",
  "license": "MIT",
  "type": "module",
  "source": "./src/index.ts",
  "types": "./dist/esm/index.d.ts",
  "main": "./dist/esm/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "yarn clean && tsc -p tsconfig.build.json --outDir dist/esm && echo '{ \"type\": \"module\" }' > dist/esm/package.json && tsc -p tsconfig.build.json --outDir dist/cjs --module CommonJS --moduleResolution Node10 && echo '{ \"type\": \"commonjs\" }' > dist/cjs/package.json",
    "clean": "rimraf dist",
    "prepack": "yarn build"
  },
  "keywords": [
    "eslint",
    "config",
    "eslint-config",
    "configuration"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aryaemami59/configs.git",
    "directory": "packages/eslint"
  },
  "bugs": {
    "url": "https://github.com/aryaemami59/configs/issues"
  },
  "peerDependencies": {
    "eslint": ">= 8.56.0"
  },
  "peerDependenciesMeta": {
    "eslint": {
      "optional": true
    }
  },
  "devDependencies": {
    "@aryaemami59/tsconfig": "workspace:^",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/eslint__js": "^8.42.3",
    "eslint": "^9.11.0",
    "rimraf": "^6.0.1",
    "tslib": "^2.7.0",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "@eslint/js": "^9.11.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.9.0",
    "typescript-eslint": "^8.6.0"
  }
}
