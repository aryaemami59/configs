    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "^8.57.0",
    "eslint-plugin-prettier": "^5.1.3",
    "rimraf": "^5.0.5",
    "tslib": "^2.6.2",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "@eslint/js": "^8.57.0",
    "@typescript-eslint/utils": "^7.2.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^14.0.0",
    "typescript-eslint": "^7.2.0"
  }
