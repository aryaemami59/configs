{
  "name": "@aryaemami59/vitest-config",
  "version": "0.0.2",
  "description": "Vitest configuration for TypeScript projects",
  "author": "Arya Emami <aryaemami59@yahoo.com> (https://github.com/aryaemami59)",
  "license": "MIT",
  "source": "./src/index.mts",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "module": "./dist/index.mjs",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "yarn clean && tsc -p tsconfig.build.json",
    "clean": "rimraf dist",
    "prepack": "yarn build"
  },
  "keywords": [
    "config",
    "vitest-config",
    "vitest",
    "vite",
    "configuration"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aryaemami59/configs.git",
    "directory": "packages/vitest"
  },
  "bugs": {
    "url": "https://github.com/aryaemami59/configs/issues"
  },
  "peerDependencies": {
    "vitest": ">= 1"
  },
  "peerDependenciesMeta": {
    "vitest": {
      "optional": true
    }
  },
  "devDependencies": {
    "@aryaemami59/tsconfig": "workspace:^",
    "rimraf": "^6.0.1",
    "tslib": "^2.7.0",
    "typescript": "^5.6.2",
    "vitest": "^2.1.1"
  },
  "dependencies": {
    "vite-tsconfig-paths": "^5.0.1"
  }
}
