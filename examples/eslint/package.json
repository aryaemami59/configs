{
  "name": "eslint-config-example",
  "version": "0.0.0",
  "private": true,
  "homepage": "https://github.com/aryaemami59/configs",
  "sideEffects": false,
  "type": "commonjs",
  "files": [],
  "scripts": {
    "clean": "rimraf $INIT_CWD/dist/ $INIT_CWD/coverage/ $INIT_CWD/temp/",
    "format-check": "yarn run -T format-check",
    "format": "yarn run -T format",
    "lint-fix": "yarn run -T lint-fix",
    "lint": "yarn run -T lint",
    "test-types": "yarn clean && tsc -p $INIT_CWD/tsconfig.json",
    "test": "yarn run -T test"
  },
  "devDependencies": {
    "@aryaemami59/eslint-config": "workspace:^",
    "@aryaemami59/tsconfig": "workspace:^",
    "@aryaemami59/vitest-config": "workspace:^",
    "@types/node": "^22.13.14",
    "eslint": "^9.23.0",
    "jiti": "^2.4.2",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.2",
    "vitest": "^3.0.9"
  }
}
